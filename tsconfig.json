{
  "include": ["src"],
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "lib": ["DOM", "ESNext"],
    "types": ["node"],
    "downlevelIteration": true
  }
}
